{
  "name": "gamespectre-client",
  "version": "1.0.0",
  "description": "A frontend for Gamespectre",
  "main": "src/server/server.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spectator-mode/client-app.git"
  },
  "keywords": [
    "youtube",
    "react",
    "node",
    "laravel",
    "api",
    "client"
  ],
  "author": "Daniel Dunderfelt",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spectator-mode/client-app/issues"
  },
  "homepage": "https://github.com/spectator-mode/client-app#readme",
  "scripts": {
    "start": "node ./node_modules/better-npm-run start",
    "build": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
    "lint": "eslint -c .eslintrc src",
    "start-dev": "node ./node_modules/better-npm-run start-dev",
    "watch-client": "node ./node_modules/better-npm-run watch-client",
    "dev": "concurrent --kill-others \"npm run watch-client\" \"npm run start-dev\"",
    "test": "karma start"
  },
  "betterScripts": {
    "start": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8080
      }
    },
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development"
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src"
      }
    }
  },
  "dependencies": {
    "axios": "^0.5.4",
    "babel": "^5.8.21",
    "babel-plugin-typecheck": "^1.2.0",
    "classnames": "^2.1.3",
    "color": "^0.10.1",
    "compression": "^1.5.1",
    "cookie-parser": "^1.3.5",
    "decorate-this": "^0.5.0",
    "es6-promise": "^2.3.0",
    "express": "^4.13.1",
    "express-session": "^1.11.3",
    "http-proxy": "^1.11.1",
    "jade": "^1.11.0",
    "lodash": "^3.10.0",
    "lru-memoize": "0.0.2",
    "map-props": "^0.1.1",
    "mobservable": "^0.6",
    "mobservable-react": "^0.1.1",
    "node-jsx": "^0.13.3",
    "object-assign": "^3.0.0",
    "piping": "0.2.0",
    "pretty-error": "^1.1.2",
    "pusher-js": "^3.0.0",
    "query-string": "^2.4.0",
    "radium": "^0.13.4",
    "react": "^0.13.3",
    "react-document-meta": "^0.1.4",
    "react-inline-css": "1.2.1",
    "react-motion": "^0.2.7",
    "react-resolver": "^2.0.4",
    "react-router": "^1.0.0-beta3",
    "react-router-server-location": "^1.0.3",
    "react-transmit": "^2.6.4",
    "serialize-javascript": "^1.0.0",
    "serve-favicon": "^2.3.0",
    "serve-static": "^1.10.0",
    "slug": "^0.9.1",
    "underscore": "^1.8.3",
    "uuid": "^2.0.1",
    "verge": "^1.9.1",
    "webpack-isomorphic-tools": "^0.8.1"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^2.2.0",
    "autoprefixer-loader": "^2.0.0",
    "babel-core": "^5.6.17",
    "babel-eslint": "^3.1.23",
    "babel-jest": "^5.3.0",
    "babel-loader": "^5.3.1",
    "babel-runtime": "^5.6.17",
    "better-npm-run": "0.0.1",
    "clean-webpack-plugin": "^0.1.3",
    "cors": "^2.7.1",
    "concurrently": "^0.1.1",
    "css-loader": "^0.15.2",
    "eslint": "^0.24.0",
    "eslint-loader": "^0.14.1",
    "eslint-plugin-react": "^2.6.4",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "json-loader": "^0.5.2",
    "jsx-loader": "^0.13.2",
    "node-libs-browser": "^0.5.2",
    "node-sass": "^3.2.0",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^1.0.2",
    "strip-loader": "^0.1.0",
    "style-loader": "^0.12.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.10.1",
    "webpack-dev-server": "^1.10.1"
  }
}
