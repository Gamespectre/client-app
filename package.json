{
  "name": "gamespectre-client",
  "version": "1.0.0",
  "description": "A frontend for Gamespectre",
  "main": "src/server/server.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spectator-mode/client-app.git"
  },
  "keywords": [
    "youtube",
    "react",
    "node",
    "laravel",
    "api",
    "client"
  ],
  "author": "Daniel Dunderfelt",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spectator-mode/client-app/issues"
  },
  "homepage": "https://github.com/spectator-mode/client-app#readme",
  "scripts": {
    "sass": "node-sass -wr -o ./src/style/css ./src/style/scss/main.scss",
    "start": "nodemon --watch src/ -e js,jade,jsx,css --exec 'jspm run' server",
    "dev": "jspm-server --no-browser --port=8080 --proxy='http://localhost:3001' --only-exts='js, jsx, css'"
  },
  "jspm": {
    "directories": {},
    "dependencies": {
      "alt": "npm:alt@^0.17.1",
      "axios": "npm:axios@^0.5.4",
      "babel-plugin-react-hot": "github:loggur/babel-plugin-react-hot@master",
      "child_process": "github:jspm/nodelibs-child_process@^0.1.0",
      "classnames": "npm:classnames@^2.1.3",
      "compression": "npm:compression@^1.5.1",
      "console": "github:jspm/nodelibs-console@^0.1.0",
      "cookie-parser": "npm:cookie-parser@^1.3.5",
      "css": "npm:jspm-loader-css@^0.1.5",
      "events": "github:jspm/nodelibs-events@^0.1.1",
      "express": "npm:express@^4.13.1",
      "fs": "github:jspm/nodelibs-fs@^0.1.2",
      "immutable": "npm:immutable@^3.7.4",
      "iso": "npm:iso@^4.1.0",
      "jade": "github:johnsoftek/plugin-jade@^0.5.0",
      "jsx": "github:loggur/plugin-babel-jsx@master",
      "object-assign": "npm:object-assign@^3.0.0",
      "path": "github:jspm/nodelibs-path@^0.1.0",
      "process": "github:jspm/nodelibs-process@^0.1.1",
      "querystring": "github:jspm/nodelibs-querystring@^0.1.0",
      "radium": "npm:radium@^0.13.2",
      "react": "npm:react@^0.13.3",
      "react-dom": "npm:react-dom@^0.14.0-beta1",
      "react-router": "npm:react-router@^0.13.3",
      "serve-favicon": "npm:serve-favicon@^2.3.0"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.1.13",
      "babel-runtime": "npm:babel-runtime@^5.1.13",
      "core-js": "npm:core-js@^0.9.4"
    }
  },
  "devDependencies": {
    "babel-eslint": "^3.1.23",
    "eslint": "^0.24.0",
    "eslint-plugin-react": "^2.6.4",
    "node-sass": "^3.2.0"
  },
  "dependencies": {}
}
