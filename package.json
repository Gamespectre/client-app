{
  "name": "gamespectre-client",
  "version": "1.0.0",
  "description": "A frontend for Gamespectre",
  "main": "src/server/server.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spectator-mode/client-app.git"
  },
  "keywords": [
    "youtube",
    "react",
    "node",
    "laravel",
    "api",
    "client"
  ],
  "author": "Daniel Dunderfelt",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spectator-mode/client-app/issues"
  },
  "homepage": "https://github.com/spectator-mode/client-app#readme",
  "scripts": {
    "start": "NODE_PATH=\"./src\" NODE_ENV=\"production\" PORT=\"8080\" node ./babel.server",
    "build": "node ./node_modules/webpack/bin/webpack.js --verbose --colors --display-error-details --config webpack/prod.config.js",
    "lint": "node ./node_modules/eslint/bin/eslint.js -c .eslintrc src",
    "start-dev": "NODE_PATH=\"./src\" NODE_ENV=\"development\" node ./babel.server",
    "watch-client": "NODE_PATH=\"./src\" node webpack/webpack-dev-server.js",
    "dev": "node ./node_modules/concurrently/src/main.js --kill-others \"npm run watch-client\" \"npm run start-dev\""
  },
  "dependencies": {
    "alt": "^0.17.1",
    "classnames": "^2.1.3",
    "compression": "^1.5.1",
    "concurrently": "^0.1.1",
    "cookie-parser": "^1.3.5",
    "es6-promise": "^2.3.0",
    "express": "^4.13.1",
    "express-session": "^1.11.3",
    "griddle-react": "^0.2.13",
    "http-proxy": "^1.11.1",
    "iso": "^4.1.0",
    "jade": "^1.11.0",
    "node-jsx": "^0.13.3",
    "object-assign": "^3.0.0",
    "piping": "^0.2.0",
    "pusher": "^1.0.6",
    "radium": "^0.13.2",
    "react": "^0.13.3",
    "react-resolver": "^1.1.7",
    "react-router": "^0.13.3",
    "serve-favicon": "^2.3.0",
    "serve-static": "^1.10.0",
    "underscore": "^1.8.3",
    "uuid": "^2.0.1"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^2.2.0",
    "autoprefixer-loader": "^2.0.0",
    "axios": "^0.5.4",
    "babel": "^5.6.14",
    "babel-core": "^5.6.17",
    "babel-eslint": "^3.1.23",
    "babel-jest": "^5.3.0",
    "babel-loader": "^5.3.1",
    "babel-plugin-typecheck": "0.0.3",
    "babel-runtime": "^5.6.17",
    "css-loader": "^0.15.2",
    "eslint": "^0.24.0",
    "eslint-loader": "^0.14.1",
    "eslint-plugin-react": "^2.6.4",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "json-loader": "^0.5.2",
    "jsx-loader": "^0.13.2",
    "node-libs-browser": "^0.5.2",
    "node-sass": "^3.2.0",
    "react-hot-loader": "^1.2.8",
    "sass-loader": "^1.0.2",
    "stats-webpack-plugin": "^0.2.1",
    "strip-loader": "^0.1.0",
    "style-loader": "^0.12.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.10.1",
    "webpack-dev-server": "^1.10.1"
  }
}
